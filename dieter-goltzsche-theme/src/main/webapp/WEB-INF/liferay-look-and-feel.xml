<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN"
"http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.0+</version>
	</compatibility>
	<theme id="dieter-goltzsche-theme" name="Dieter Goltzsche Theme">
		<settings>
			<setting configurable="true" key="display-footer-slogan" type="checkbox" />
			<setting configurable="true" key="footer-slogan" />
			<setting configurable="true" key="portlet-setup-show-borders-default" type="checkbox" value="false" />
			<setting configurable="false" key="show-site-name-default" value="false" />
			<setting configurable="false" key="show-site-name-supported" value="false" />
		</settings>
	</theme>
</look-and-feel>
